{
  "name": "@gambonny/valext",
  "author": "Jonny Gamba",
  "version": "0.1.0",
  "sideEffects": true,
  "description": "Minimal extraction and validation helper for Valibot.",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "format": "pnpm exec biome format --write",
    "lint": "pnpm exec biome lint",
    "build": "tsdown",
    "typecheck": "tsc --noEmit",
    "test": "vitest"
  },
  "keywords": [
    "valibot",
    "extract",
    "fluent-api",
    "runtime-validation"
  ],
  "license": "ISC",
  "packageManager": "pnpm@10.13.1",
  "devDependencies": {
    "@biomejs/biome": "2.1.2",
    "lefthook": "^1.12.2",
    "tsdown": "^0.13.0",
    "typescript": "^5.8.3",
    "valibot": "^1.1.0",
    "vitest": "^3.2.4"
  },
  "peerDependencies": {
    "valibot": "^1.1.0"
  },
  "files": [
    "dist"
  ]
}
